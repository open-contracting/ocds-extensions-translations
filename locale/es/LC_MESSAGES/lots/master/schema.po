msgstr ""

#. /definitions/LotGroup/title
#: release-schema.json:1
msgid "Lot group"
msgstr "Grupo del lote"

#. /definitions/LotGroup/properties/id/title
#: release-schema.json:1
msgid "Lot group identifier"
msgstr "Identificador del grupo de lote"

#. /definitions/LotGroup/properties/id/description
#: release-schema.json:1
msgid "A local identifier for this group of lots."
msgstr "Un identificador local para este grupo de lotes."

#. /definitions/LotGroup/properties/identifiers/description
#: release-schema.json:1
msgid "Identifiers for this lot group. This field can be used to provide internal identifiers for the lot group, such as identifiers from a buyer's document management system or procurement system."
msgstr ""

#. /definitions/LotGroup/properties/title/description
#: release-schema.json:1
msgid "A title for this lot group."
msgstr ""

#. /definitions/LotGroup/properties/description/description
#: release-schema.json:1
msgid "A description of this lot group."
msgstr ""

#. /definitions/LotGroup/properties/relatedLots/description
#: release-schema.json:1
msgid "A list of the identifiers of the lots that form this group. Lots can appear in more than one group."
msgstr "Una lista de los identificadores de los lotes que forman este grupo. Los lotes pueden aparecer en más de un grupo."

#. /definitions/LotGroup/properties/optionToCombine/title
#: release-schema.json:1
msgid "Option to combine"
msgstr "Opción de combinar"

#. /definitions/LotGroup/properties/optionToCombine/description
#: release-schema.json:1
msgid "The buyer reserves the right to combine the lots in this group when awarding a contract."
msgstr "El comprador se reserva el derecho de combinar los lotes de este grupo al adjudicar un contrato."

#. /definitions/LotGroup/properties/maximumValue/title
#: release-schema.json:1
msgid "Maximum value"
msgstr "Valor máximo"

#. /definitions/LotGroup/properties/maximumValue/description
#: release-schema.json:1
msgid "The maximum estimated value of the lots in this group. This can be lower than the sum total of lot values"
msgstr "El valor máximo estimado de los lotes de este grupo. Este puede ser inferior a la suma total de los valores de los lotes."

