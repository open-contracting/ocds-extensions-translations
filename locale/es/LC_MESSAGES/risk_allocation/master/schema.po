#. /name/en
#: extension.json:1
msgid "Risk Allocation"
msgstr "Asignación de Riesgo"

#. /description/en
#: extension.json:1
msgid "For providing the risk allocations defined in a public private partnership's contract."
msgstr "Para proporcionar las asignaciones de riesgo definidas en un contrato de asociación público-privada."

#. /definitions/Risk/title
#: release-schema.json:1
msgid "Risk"
msgstr "Riesgo"

#. /definitions/Risk/description
#: release-schema.json:1
msgid "A risk relating to the project."
msgstr "Un riesgo relacionado al proyecto."

#. /definitions/Risk/properties/id/title
#: release-schema.json:1
msgid "Risk ID"
msgstr "ID de riesgo"

#. /definitions/Risk/properties/id/description
#: release-schema.json:1
msgid "A local identifier for this risk, unique within this block. This field is used to keep track of multiple revisions of a risk through the compilation from release to record mechanism."
msgstr "Un identificador local para este riesgo, único en este bloque. Este campo se utiliza para mantener el monitoreo de revisiones múltiples de un riesgo a través de la compilación desde la entrega hasta el registro."

#. /definitions/Risk/properties/category/title
#: release-schema.json:1
msgid "Risk category"
msgstr "Categoría de riesgo"

#. /definitions/Risk/properties/category/description
#: release-schema.json:1
msgid "The category of the risk, from the riskCategory codelist."
msgstr "La categoría del riesgo, de la lista de códigos riskCategory."

#. /definitions/Risk/properties/description/title
#: release-schema.json:1
msgid "Risk description"
msgstr "Descripción de riesgo"

#. /definitions/Risk/properties/description/description
#: release-schema.json:1
msgid "The description of the risk."
msgstr "La descripción del riesgo."

#. /definitions/Risk/properties/allocation/title
#. /definitions/Contract/properties/riskAllocation/title
#: release-schema.json:1
msgid "Risk allocation"
msgstr "Asignación de riesgos"

#. /definitions/Risk/properties/allocation/description
#: release-schema.json:1
msgid "The party to the contract that retains the risk."
msgstr "La parte del contrato que retiene el riesgo."

#. /definitions/Risk/properties/notes/title
#: release-schema.json:1
msgid "Risk notes"
msgstr "Notas de Riesgo"

#. /definitions/Risk/properties/notes/description
#: release-schema.json:1
msgid "Additional notes on the risk, for example the rationale for the allocation."
msgstr "Notas adicionales sobre el riesgo, por ejemplo la justificación de la asignación."

#. /definitions/Contract/properties/riskAllocation/description
#: release-schema.json:1
msgid "Information on the allocation of risks relating to the contract."
msgstr "Información sobre la asignación de los riesgos relacionados con el contrato."

